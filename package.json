  "main": "index.js",
  "scripts": {
    "sass-prod" : "sass --watch ./sass/main.scss:./public/css/style.css --style=compressed",
    "sass-dev" : "sass --watch ./sass/main.scss:./public/css/style.css",
    "sass-alafrancaise-dev" : "sass --watch ./sass/alafrancaise.scss:./public/css/alafrancaise.css",
    "sass-alafrancaise-prod" : "sass --watch ./sass/alafrancaise.scss:./public/css/alafrancaise.css --style=compressed",
    "prefix": "postcss public/css/style.css --use autoprefixer -d public/css/prefixed/",
    "prefix-alafrancaise": "postcss public/css/alafrancaise.css --use autoprefixer -d public/css/prefixed/"
  },
